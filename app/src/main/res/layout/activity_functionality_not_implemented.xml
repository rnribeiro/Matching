<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FunctionalityNotImplemented">

    <ImageView
            android:src="@drawable/logo"
            android:layout_width="215dp"
            android:layout_height="50dp"
            android:id="@+id/logo_img_view2"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.023"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

    <ImageButton
            android:src="@drawable/menu_icon"
            android:layout_width="55dp"
            android:layout_height="50dp"
            android:id="@+id/menu_icon_btn2"
            android:background="#00FFFFFF"
            tools:ignore="SpeakableTextPresentCheck"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.081"
            app:layout_constraintVertical_bias="0.023"/>

    <ImageView
            android:layout_width="223dp"
            android:layout_height="232dp"
            app:srcCompat="@drawable/cancel"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="94dp"
            android:layout_marginEnd="94dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.563"/>

    <TextView
            android:text=" Functionality not implemented"
            android:layout_width="348dp"
            android:layout_height="148dp"
            android:id="@+id/textView3"
            android:textSize="35sp"
            android:gravity="center_horizontal|center_vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintVertical_bias="0.169"/>

    <Button
            android:text="Close"
            android:layout_width="125dp"
            android:layout_height="55dp"
            android:id="@+id/not_implemented_btn"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.877"
            android:backgroundTint="#37A82A" android:textSize="16sp"/>

</androidx.constraintlayout.widget.ConstraintLayout>